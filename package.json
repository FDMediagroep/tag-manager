{
    "name": "fdmg-tag-manager",
    "version": "1.0.0",
    "description": "a GUI for managing JavaScript snippets",
    "main": "index.js",
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "docker:build": "docker build -t fdmg-tag-manager .",
        "docker:logs": "docker logs fdmg-tag-manager",
        "docker:pull": "docker pull ghcr.io/fdmediagroep/fdmg-tag-manager:latest",
        "docker:run": "docker run -d -p 3000:3000 --env-file .env --name fdmg-tag-manager fdmg-tag-manager",
        "docker:restart": "docker restart fdmg-tag-manager",
        "docker:stop": "docker stop fdmg-tag-manager && docker rm fdmg-tag-manager && docker image prune -f",
        "docker:tag": "docker tag ghcr.io/fdmediagroep/fdmg-tag-manager:latest fdmg-tag-manager:latest",
        "lint": "eslint \"src/**\"",
        "prettier": "prettier -c **/*.ts?",
        "start": "next start",
        "test": "echo \"Error: no test specified\" && exit 1",
        "vc:dev": "vc dev",
        "vc:rm": "vc rm tag-manager --safe --yes"
    },
    "author": "Willem Liu",
    "license": "ISC",
    "devDependencies": {
        "@fdmg/design-system": "^0.4.42",
        "@types/basic-auth": "^1.1.3",
        "@types/next-auth": "^3.15.0",
        "@types/node": "^15.6.1",
        "@types/raw-body": "^2.3.0",
        "@types/react": "^17.0.8",
        "@types/react-dom": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "@typescript-eslint/typescript-estree": "^4.25.0",
        "aws-sdk": "^2.914.0",
        "basic-auth": "^2.0.1",
        "date-input-polyfill": "^2.14.0",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "jshint": "^2.13.0",
        "next": "^10.2.3",
        "next-auth": "^3.23.3",
        "prettier": "^2.3.0",
        "raw-body": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "sass": "^1.34.0",
        "typescript": "^4.2.4",
        "uuid": "^8.3.2",
        "vercel": "^23.0.0"
    }
}
